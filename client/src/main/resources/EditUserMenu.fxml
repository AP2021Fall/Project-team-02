<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.ListView?>
<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.fx.team.leader.EditUserController">
   <children>
      <Label alignment="CENTER" layoutY="-1.0" prefHeight="36.0" prefWidth="500.0" style="-fx-background-color: #1F8FE8;" text="MEMBERS" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>


      <TextField fx:id="UsernameData" layoutX="180.0" layoutY="50.0" prefHeight="26.0" prefWidth="167.0" promptText="username ..."/>
      <TextField fx:id="NewUsernameData" layoutX="180.0" layoutY="100.0" prefHeight="26.0" prefWidth="167.0" promptText="new username ..."/>
      <TextField fx:id="EmailData" layoutX="180.0" layoutY="150.0" prefHeight="26.0" prefWidth="167.0" promptText="new email ..."/>
      <TextField fx:id="PasswordData" layoutX="180.0" layoutY="200.0" prefHeight="26.0" prefWidth="167.0" promptText="new password ..."/>

      <Button fx:id="EditUserButton" layoutX="300.0" layoutY="490.0" mnemonicParsing="false" onAction="#onClick_EditButton" prefHeight="26.0" prefWidth="135.0" style="-fx-background-color: #1F8FE8;" text="EDIT USER" textFill="#f2f2f2">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="BackButton" layoutX="100.0" layoutY="490.0" mnemonicParsing="false" onAction="#onClick_BackButton" prefHeight="26.0" prefWidth="135.0" style="-fx-background-color: #1F8FE8;" text="BACK" textFill="#f2f2f2">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
