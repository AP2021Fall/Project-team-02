<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.ListView?>
<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.fx.team.share.SelectTeamController">
   <children>
      <Label alignment="CENTER" layoutY="-1.0" prefHeight="36.0" prefWidth="500.0" style="-fx-background-color: #1F8FE8;" text="SELECT TEAM" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>

      <Label layoutX="70.0" layoutY="159.0" text="Enter New Username:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>


      <TextField fx:id="TeamName" layoutX="180.0" layoutY="50.0" prefHeight="26.0" prefWidth="167.0"  promptText="enter selected team name"/>
      <ListView fx:id="TeamListView" layoutX="50.0" layoutY="100.0" prefHeight="300.0" prefWidth="400" />
      <Button fx:id="SelectButton" layoutX="300.0" layoutY="400.0" mnemonicParsing="false" onAction="#onClick_SelectButton" prefHeight="26.0" prefWidth="98.0" style="-fx-background-color: #1F8FE8;" text="SELECT" textFill="#f2f2f2">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="CancelButton" layoutX="100.0" layoutY="400.0" mnemonicParsing="false" onAction="#onClick_CancelButton" prefHeight="26.0" prefWidth="98.0" style="-fx-background-color: #1F8FE8;" text="BACK" textFill="#f2f2f2">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
